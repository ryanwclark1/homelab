# N8N Configuration
# Copy this file to .env and update with your values

##### SYSTEM
TZ=Etc/UTC

##### VERSIONS
N8N_TAG=1.42.1
POSTGRES_TAG=16-alpine
POSTGRES_BACKUP_TAG=latest

##### PORTS
N8N_PORT=5678
N8N_POSTGRES_PORT=5432

##### DATABASE
N8N_DB_NAME=n8n
N8N_DB_USER=n8n
N8N_DB_PASSWORD=changeme_strong_password_here

##### N8N AUTHENTICATION
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=changeme_admin_password_here

##### N8N CONFIGURATION
N8N_WEBHOOK_URL=https://n8n.example.com/

##### BACKUP CONFIGURATION
BACKUP_SCHEDULE=@daily
BACKUP_KEEP_DAYS=7
BACKUP_KEEP_WEEKS=4
BACKUP_KEEP_MONTHS=6
